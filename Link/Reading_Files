#Selenium4 Read Data / do not support excel sheets
#openpyxl - model to work with excel sheets
from selenium import webdriver
import openpyxl

path="/Users/quratulainawais/PycharmProjects/PythonProject/SeleniumDemo/.venv/Screen/data.xlsx"
work_book=openpyxl.load_workbook(path)
sheet=work_book['data']
row=sheet.max_row
col=sheet.max_column
#Read File per row

for r in range(1,row+1):
    for c in range(1,col+1):
        print(sheet.cell(row=r,column=c).value, end='    ')
    print() # Next line
    








