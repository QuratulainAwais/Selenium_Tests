#Selenium4 Add data to files
#openpyxl - model to work with excel sheets
from selenium import webdriver
import openpyxl

path="/Users/quratulainawais/PycharmProjects/PythonProject/SeleniumDemo/.venv/Screen/Add.xlsx"
work_book=openpyxl.load_workbook(path)
sheet=work_book.active #Incase you have one sheet only
for r in range(1,6):
    for c in range(1,4):
        sheet.cell(row=r, column=c, value="welcome")
work_book.save(path)

